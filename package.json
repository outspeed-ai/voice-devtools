{
  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.app.json --env-file=.env src/server.ts",
    "start": "tsx src/server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc -b && vite build --outDir dist/client --ssrManifest",
    "build:server": "tsc -b && vite build --outDir dist/server --ssr ./src/client/entry-server.jsx",
    "devinstall": "zx ../../devinstall.mjs -- node src/server.js --dev",
    "lint": "eslint . --ext .js,.jsx --fix",
    "analyze": "ANALYZE=true npm run build:client",
    "deploy": "node scripts/deploy.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/vite": "^4.1.1",
    "@tanstack/react-query": "^5.66.9",
    "axios": "^1.7.9",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "figlet": "^1.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router": "^7.4.0",
    "react-syntax-highlighter": "^15.6.1",
    "semver": "^7.7.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250327.0",
    "@eslint/js": "^9.22.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "node-pty": "^1.0.0",
    "postcss": "^8.4.31",
    "postcss-nesting": "^12.0.2",
    "postcss-preset-env": "^7.7.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^6.2.2",
    "wrangler": "^4.12.0"
  }
}
